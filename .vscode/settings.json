{
  "files.associations": {
    "*.mal": "clojure",
    "*.asm": "gbz80",
    "*.json": "jsonc",
    "*.inc": "php",
    "*.tsx": "xml",
    "*.h": "cpp",
    "*.rh": "cpp",
    "xiosbase": "cpp",
    "filesystem": "cpp",
    "string": "cpp"
  },
  "editor.rulers": [
    80
  ],
  "editor.indentSize": 4,
  "editor.formatOnSave": false,
  "editor.formatOnPaste": true,
  "cmake.debugConfig": {
    "cwd": "${workspaceFolder}/build/runtime/"
  }
}